<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_voting_process_import_wizard" model="ir.actions.act_window">
        <field name="name">Importar Procesos de Votación</field>
        <field name="res_model">voting.process.import.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record id="view_voting_process_import_wizard_form" model="ir.ui.view">
        <field name="name">voting.process.import.wizard.form</field>
        <field name="model">voting.process.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Importar Procesos de Votación">
                <group>
                    <field name="file" filename="filename"/>
                </group>
                <footer>
                    <button name="import_processes" type="object" string="Importar" class="btn-primary"/>
                    <button name="download_template" type="object" string="Descargar plantilla ejemplo" class="btn-secondary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
